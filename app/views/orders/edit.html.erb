<%= form_with model: @order, url: order_path(@order.id) do |f| %>
<p> Edit Order </p>
<% @order.order_items.each do |item| %>
      Item: <%= item.id %>
    <%= f.label :quantity %>
    <%= f.number_field :quantity, value: item.qty %>
    <%= f.label :remove %>
    <%= f.check_box :remove %>
  <%= f.submit "Update" %>
  <% end %>
<% end %>
