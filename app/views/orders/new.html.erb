<%=form_with model: @order, class: 'create-new-order' do |f|%>
  <%= f.label :name%>
  <%= f.text_field :name%>

  <%= f.label :address%>
  <%= f.text_field :address%>

  <%= f.label :email%>
  <%= f.text_field :email%>

  <%= f.label :credit_card_num%>
  <%= f.text_field :credit_card_num%>

  <%= f.label :cvv%>
  <%= f.text_field :cvv%>

  <%= f.label :zip%>
  <%= f.text_field :zip%>

  <%= f.label :status%>
  <%= f.text_field :status%>

  <%= f.label :total_price%>
  <%= f.text_field :total_price%>

  <%= f.label :placed_time%>
  <%= f.text_field :placed_time%>

  <%= f.label :expiration_date%>
  <%= f.text_field :expiration_date%>

  <%= f.submit "Complete order", :class => "btn btn-primary"%>
<%end%>