<h2 class="product-title">Our Products</h2>

<section class="index-product-container">
<% @products.each do |product| %>
  <%= image_tag product.photo_url, alt: "image of product" %> <br>
  <h3><%= product.name %></h3>
  $<%= product.price %>
<% end %>
</section>

