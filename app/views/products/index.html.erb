<h1>Products</h1>
<% @products.each do |product| %>
  <% if product.retire == false %>
    <%= link_to product.name, product_path(product.id)%>
    <%= button_to 'Add to Cart', cart_path(product.id), method: :get%>
  <% end %>
<!--  <a class="badge badge-pill badge-info" href="/cart/:id">Add to Cart</a> -->
<% end %>